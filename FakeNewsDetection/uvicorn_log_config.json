{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "file_formatter": {
            "format": "%(asctime)s - App - %(levelname)s - %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "file_handler": {
            "class": "logging.FileHandler",
            "filename": "logs.txt",
            "formatter": "file_formatter"
        }
    },
    "loggers": {
        "uvicorn": {
            "handlers": [
                "file_handler"
            ],
            "level": "INFO"
        }
    }
}